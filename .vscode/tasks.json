{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "ETNA: Build Project",
			"type": "shell",
			"command": "python",
			"args": [
				"etna_cli.py",
				"build"
			],
			"group": "build",
			"presentation": {
				"echo": true,
				"reveal": "always",
				"focus": false,
				"panel": "shared"
			},
			"problemMatcher": []
		},
		{
			"label": "ETNA: Build Release",
			"type": "shell",
			"command": "python",
			"args": [
				"etna_cli.py",
				"build",
				"--release"
			],
			"group": "build",
			"presentation": {
				"echo": true,
				"reveal": "always",
				"focus": false,
				"panel": "shared"
			},
			"problemMatcher": []
		},
		{
			"label": "ETNA: Run Tests",
			"type": "shell",
			"command": "python",
			"args": [
				"etna_cli.py",
				"test"
			],
			"group": "test",
			"presentation": {
				"echo": true,
				"reveal": "always",
				"focus": false,
				"panel": "shared"
			},
			"problemMatcher": []
		},
		{
			"label": "ETNA: Train Sample Model",
			"type": "shell",
			"command": "python",
			"args": [
				"etna_cli.py",
				"train",
				"--data",
				"examples/sample_data.json",
				"--epochs",
				"50"
			],
			"group": "build",
			"presentation": {
				"echo": true,
				"reveal": "always",
				"focus": false,
				"panel": "shared"
			},
			"problemMatcher": []
		},
		{
			"label": "ETNA: Make Predictions",
			"type": "shell",
			"command": "python",
			"args": [
				"etna_cli.py",
				"predict",
				"--data",
				"examples/test_data.json"
			],
			"group": "build",
			"presentation": {
				"echo": true,
				"reveal": "always",
				"focus": false,
				"panel": "shared"
			},
			"problemMatcher": []
		},
		{
			"label": "ETNA: Clean Build",
			"type": "shell",
			"command": "python",
			"args": [
				"etna_cli.py",
				"clean"
			],
			"group": "build",
			"presentation": {
				"echo": true,
				"reveal": "always",
				"focus": false,
				"panel": "shared"
			},
			"problemMatcher": []
		}
	]
}